          },
          {
            "name": "002",
            "kind": "SPARSE_FC",
            "outputs": 50,
            "def": {
              "sparsity": 0,
              "greediness": 5.01
            }
          },
          {
            "name": "002.1",
            "kind": "LRELU",
            "def": {
              "leakiness": 50
            }
          },
          {
            "name": "003",
            "kind": "SPARSE_FC",
            "outputs": 25,
            "def": {
              "sparsity": 0
            }
          },
          {
            "name": "003.1",
            "kind": "LRELU",
            "def": {
              "leakiness": 50
            }
          },
          {
            "name": "004",
