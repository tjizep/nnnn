          {
            "name": "009",
            "kind": "SOFTMAX",
            "def": {}
          }
        ]
      }
    },
    "optimizer": {
      "mnist_good": {
        "kind": "SGD",
        "def": {
          "epochs": 24,
          "mini_batch_size": 24,
          "threads": 8,
          "learning_rates": [
            0.009,
            0.0001
          ],
          "save_schedule": [
            0.7,
            0.8,
            0.95,
            0.98
          ]
        }
      }
    }
  }
}